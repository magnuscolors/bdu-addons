<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<!-- team dashboard Alkmaar -->
    <record id="team_dashboard_alkmaar" model="ir.ui.view">
        <field name="name">Team dashboard Alkmaar</field>
        <field name="model">board.board</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
        	<form string="Team dashboard Alkmaar">
              <h2>Team dashboard Alkmaar</h2>
            	<board style="2-1">
                        <column>
                        </column>
            	</board>
        	</form>
        </field>
    </record>

    <!-- Action  --> 
  	<act_window id="team_dashboard_alkmaar_action" 
              name="Team dashboard Alkmaar" 
              res_model="board.board" 
              view_mode="form" 
              view_id="team_dashboard_alkmaar"
    /> 

    <!-- menuitem -->
    <menuitem 
    	id="team_dashboard_alkmaar_menuitem" 
     	name="Team dashboard Alkmaar"
        parent="base.menu_board_root" 
        sequence="6"
        action="team_dashboard_alkmaar_action"
        groups="team_alkmaar" 
    />




